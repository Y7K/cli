{
    "name": "y7k/cli",
    "description": "The Y7K CLI",
    "keywords": [
        "cli"
    ],
    "homepage": "http://y7k.com",
    "type": "project",
    "license": "UNLICENSE",
    "require": {
        "php": ">=7.1.3",
        "laravel-zero/framework": "^8.6",
        "laravelcollective/remote": "^6.2",
        "symfony/yaml": "^4.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "y7k"
    ],
    "version": "5.0.0"
}
